{
    "name": "metaregistrar\/api-client",
    "version": "1.0.13",
    "description": "Provides connectivity with the Metaregistrar rest api",
    "license": "proprietary",
    "keywords": [
        "domain",
        "domain registration",
        "dns",
        "registry"
    ],
    "authors": [
        {
            "name": "John Bakker",
            "email": "john@metaregistrar.com"
        },
        {
            "name": "Erwin Maliepaard",
            "email": "erwin@metaregistrar.com"
        }
    ],
    "require": {
        "php": "~7.4|~8.2",
        "ext-intl": "*",
        "ext-json": "*",
        "doctrine\/annotations": "^1.0|^2.0",
        "guzzlehttp\/guzzle": "~6|^7.2",
        "jms\/serializer": "^3.1",
        "monolog\/monolog": "^3.7",
        "webdevvie\/nestis": "^1.0"
    },
    "require-dev": {
        "mikey179\/vfsstream": "^1.6",
        "mockery\/mockery": "^1.3",
        "phpmd\/phpmd": "^2.8.2",
        "phpstan\/phpstan": "^1.10",
        "phpunit\/phpunit": "^9.6",
        "squizlabs\/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Metaregistrar\\Api\\Client\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Metaregistrar\\Api\\Client\\Tests\\": "Tests\/"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "audit": {
            "abandoned": "report"
        }
    }
}